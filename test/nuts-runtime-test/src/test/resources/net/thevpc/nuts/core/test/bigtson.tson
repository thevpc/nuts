page-group{
  assign("value" : classic// use classic or ibtihel
, "name" : "themeName")  ,
  assign("value" : 1, "name" : "documentColorAccent")  ,
  assign("value" : true , "name" : "themeShowPalette")  ,
  assign("value" : "ntexup" , "name" : "documentTitle")  ,
  assign(
    "name" : "documentSubtitle" , "value" : "
       A declarative, text-based document
              & presentation generator
                 "
  )  ,
  assign("name" : "documentSubsubtitle" , "value" : "Open Source")  ,
  assign("value" : "thevpc" , "name" : "documentAuthor")  ,
  assign("name" : "documentAuthorEmail" , "value" : "https://github.com/thevpc/ntexup")  ,
  assign("value" : "thevpc" , "name" : "documentAuthorAffiliation")  ,
  assign("value" : "Chapter 1" , "name" : "documentChapter")  ,
  assign("name" : "documentDate" , "value" : "2025-08-06")  ,
  assign("name" : "documentVersion" , "value" : "v0.8.9.0")  ,
  assign(
    "name" : "documentColors" , "value" : [
    "#26355D"  ,
    "#AF47D2"  ,
    "#DE3163"  ,
    "#1d8147"  ,
    "#FF8F00"  ,
    "#FF7F50"  ,
    "#FFBF00"  ,
    "#FFDB00"  ,
    "#9FE2BF"  ,
    "#40E0D0"  ,
    "#CCCCFF"  ,
    "#6495ED"
  ]
  )  ,
  styles{
    (*){font-size : 5%P , debug-color : red , font-family : "Serif"}  ,
    source{
      font-size : 3%P  ,
      font-family : "monospaced"  ,
      source-primary-color : documentColors  ,
      source-keyword-color : documentColors[1]  ,
      source-separator-color : documentColors[6]  ,
      source-string-color : documentColors[4]  ,
      source-comments-color : documentColors[12]  ,
      source-number-color : documentColors[3]  ,
      source-keyword-font-bold//draw-contour,

    }  ,
    page{background : Gray93} ,
    (.ul-item){margin : (5, 0) , at : left} ,
    (.ul-bullet){background : documentColors[1] , at : left , size : 2%P} ,
    (.ol-bullet){color : documentColors[2] , at : center} ,
    (.ol-item){at : left}  ,
    ellipse{at : center}  ,
    arc{at : center} ,
    (.image-fig){margin : 1} ,
    (.image-fig-legend){at : bottom , font-size : 3%P} ,
    (.image-fig-title){at : top , font-size : 4%P , font-bold , color : documentColors[4]}
  } ,
  @definePalette{
    group(size : 100%P){
      rectangle(
        origin : top-left , position : (0.5, 1) , size : (1, 2) , background : documentColors[1] , rotate : 0 , color : gray , draw-contour
      )  ,
      rectangle(
        origin : top-left , position : (0.5, 3) , size : (1, 2) , background : documentColors[2] , color : gray , rotate : 0 , draw-contour
      )  ,
      rectangle(
        origin : top-left , position : (0.5, 5) , size : (1, 2) , background : documentColors[3] , rotate : 0 , color : gray , draw-contour
      )  ,
      rectangle(
        origin : top-left , position : (0.5, 7) , size : (1, 2) , background : documentColors[4] , rotate : 0 , color : gray , draw-contour
      )  ,
      rectangle(
        origin : top-left , position : (0.5, 9) , size : (1, 2) , background : documentColors[5] , rotate : 0 , color : gray , draw-contour
      )  ,
      rectangle(
        origin : top-left , position : (0.5, 11) , size : (1, 2) , background : documentColors[6] , rotate : 0 , color : gray , draw-contour
      )  ,
      rectangle(
        origin : top-left , position : (1.5, 1.5) , size : (1, 2) , background : documentColors[7] , rotate : 0 , color : gray , draw-contour
      )  ,
