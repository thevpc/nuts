<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <actionName>run</actionName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:1.5.0:exec</goal>
        </goals>
        <properties>
            <exec.args>"-Dnuts.export.debug=true" "-Dnuts.boot.args=-t -y" "-Dnuts.export.debug=true" "-Dnuts.export.always-show-command=true" -classpath %classpath net.vpc.app.nuts.toolbox.nsh.Nsh -c json -f /home/vpc/.config/nuts/default-workspace/nuts-workspace.json -q .</exec.args>
            <exec.executable>java</exec.executable>
            <exec.workingdir></exec.workingdir>
        </properties>
    </action>
    <action>
        <actionName>debug</actionName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:1.5.0:exec</goal>
        </goals>
        <properties>
            <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} "-Dnuts.export.debug=true" "-Dnuts.boot.args=-t -y" "-Dnuts.export.debug=true" "-Dnuts.export.always-show-command=true" -classpath %classpath net.vpc.app.nuts.toolbox.nsh.Nsh -c json -f /home/vpc/.config/nuts/default-workspace/nuts-workspace.json -q .</exec.args>
            <exec.executable>java</exec.executable>
            <jpda.listen>true</jpda.listen>
            <exec.workingdir></exec.workingdir>
        </properties>
    </action>
    <action>
        <actionName>profile</actionName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:1.5.0:exec</goal>
        </goals>
        <properties>
            <exec.args>"-Dnuts.export.debug=true" "-Dnuts.boot.args=-t -y" "-Dnuts.export.debug=true" "-Dnuts.export.always-show-command=true" -classpath %classpath net.vpc.app.nuts.toolbox.nsh.Nsh -c json -f /home/vpc/.config/nuts/default-workspace/nuts-workspace.json -q .</exec.args>
            <exec.executable>java</exec.executable>
            <exec.workingdir></exec.workingdir>
        </properties>
    </action>
</actions>
