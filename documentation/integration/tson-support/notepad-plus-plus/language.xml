<?xml version="1.0" encoding="Windows-1252" ?>
<NotepadPlus>
    <UserLang name="TSON" ext="tson">
        <Settings>
            <Global caseIgnored="no" allowFoldOfComments="yes" foldCompact="yes" />
            <TreatAsSymbol commentLine="//" commentStart="/*" commentEnd="*/" />
        </Settings>

        <KeywordLists>
            <Keywords name="Keywords1"></Keywords>
            <Keywords name="Keywords2"></Keywords>
            <Keywords name="Keywords3"></Keywords>
            <Keywords name="Keywords4"></Keywords>
            <Keywords name="Keywords5"></Keywords>
            <Keywords name="Keywords6"></Keywords>
            <Keywords name="Keywords7"></Keywords>
            <Keywords name="Keywords8"></Keywords>
        </KeywordLists>

        <Styles>
            <WordsStyle name="DEFAULT" fgColor="000000" bgColor="FFFFFF" fontName="Consolas" fontStyle="0" fontSize="10" />
            <WordsStyle name="KEYWORD1" fgColor="0000FF" fontStyle="1" />
            <WordsStyle name="KEYWORD2" fgColor="008000" fontStyle="1" />
            <WordsStyle name="KEYWORD3" fgColor="800080" fontStyle="1" />
            <WordsStyle name="COMMENT" fgColor="808080" fontStyle="2" />
            <WordsStyle name="NUMBER" fgColor="FF0000" fontStyle="0" />
            <WordsStyle name="STRING" fgColor="008000" fontStyle="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
